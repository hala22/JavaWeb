<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>javaWeb</display-name>
  
  <!-- 欢迎列表，如果工程中有一下名称文件自动匹配，从上到下进行，如果都没有显示404 -->
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  		<servlet-name>doS1</servlet-name>
  		<servlet-class>javaWeb.ServlerDemo1</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>doS1</servlet-name>
  		<url-pattern>/doS1</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  		<servlet-name>show</servlet-name>
  		<servlet-class>javaWeb.ShowServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>show</servlet-name>
  		<url-pattern>/doShow</url-pattern>
  </servlet-mapping>
  
  <servlet>
  		<servlet-name>add</servlet-name>
  		<servlet-class>javaWeb.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>add</servlet-name>
  		<url-pattern>/doAdd</url-pattern>
  </servlet-mapping>
  
   <servlet>
  		<servlet-name>cart</servlet-name>
  		<servlet-class>javaWeb.CartServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>cart</servlet-name>
  		<url-pattern>/doCart</url-pattern>
  </servlet-mapping>
</web-app>